    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-confetti": "^6.1.0",
    "react-custom-roulette": "^1.4.1",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
